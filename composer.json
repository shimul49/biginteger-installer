{
    "name": "biginteger/laravel-installer",
    "description": "A beautiful Laravel installer package",
    "type": "library",
    "license": "MIT",
    "version": "1.0",
    "autoload": {
        "psr-4": {
            "BigInteger\\LaravelInstaller\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BigInteger\\LaravelInstaller\\InstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
} 